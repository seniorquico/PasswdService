{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "restore",
      "type": "process",
      "command": "dotnet",
      "args": ["restore", "${workspaceFolder}/PasswdService.sln", "/property:GenerateFullPaths=true"],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "build",
      "type": "process",
      "command": "dotnet",
      "args": ["build", "${workspaceFolder}/PasswdService.sln", "/property:GenerateFullPaths=true"],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "clean",
      "type": "process",
      "command": "dotnet",
      "args": ["clean", "${workspaceFolder}/PasswdService.sln", "/property:GenerateFullPaths=true"],
      "problemMatcher": "$msCompile",
      "group": "build"
    }
  ]
}
